{
	"name" : "amylian/yii2-amylian-phpunit",
	"description" : "ATTENTION EXPERIMENTAL! Amylian/PhpUnit for Yii2 - Utilities for easy Yii2 PhpUnit Tests",
	"keywords" : [
		"yii2",
		"phpunit"
	],
	"type" : "yii2-extension",
	"license" : "BSD-3-Clause",
	"authors" : [{
			"name" : "Andreas Prucha (Abexto - Helicon Software Development - Andreas Prucha / Amylian Project)",
			"email" : "andreas.prucha@gmail.com",
			"homepage" : "http://www.abexto.com",
			"role" : "Developer"
		}
	],
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}
	],
	"require" : {
		"yiisoft/yii2" : "~2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "<6"
	},
	"autoload" : {
		"psr-4" : {
			"amylian\\yii\\phpunit\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"amylian\\yii\\phpunit\\tests\\" : "tests/"
		}
	}
}